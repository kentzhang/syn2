{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"workspaceName": {
			"type": "string",
			"metadata": "Workspace name",
			"defaultValue": "xltestscus"
		},
		"xltestscus-WorkspaceDefaultSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'xltestscus-WorkspaceDefaultSqlServer'"
		},
		"gen2SpnCred_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://xlgen2.dfs.core.windows.net"
		},
		"xl_adf_akv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://xl-adf-akv.vault.azure.net/"
		},
		"xltestscus-WorkspaceDefaultStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://xltestdlg2.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"workspaceId": "[concat('Microsoft.Synapse/workspaces/', parameters('workspaceName'))]"
	},
	"resources": [
		{
			"name": "xltestscus_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"workspaceName": {
						"value": "[parameters('workspaceName')]"
					},
					"xl_adf_akv_properties_typeProperties_baseUrl": {
						"value": "[parameters('xl_adf_akv_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "xltestscus_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"workspaceName": {
						"value": "[parameters('workspaceName')]"
					},
					"xltestscus-WorkspaceDefaultSqlServer_connectionString": {
						"value": "[parameters('xltestscus-WorkspaceDefaultSqlServer_connectionString')]"
					},
					"xltestscus-WorkspaceDefaultStorage_properties_typeProperties_url": {
						"value": "[parameters('xltestscus-WorkspaceDefaultStorage_properties_typeProperties_url')]"
					},
					"gen2SpnCred_properties_typeProperties_url": {
						"value": "[parameters('gen2SpnCred_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'xltestscus_ArmTemplate_0')]"
			]
		}
	]
}