			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'xldevscus-WorkspaceDefaultSqlServer'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://CIP-AzNS-Secerts.vault.azure.net/"
		},
		"xldevscus-WorkspaceDefaultStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://xlgen2.dfs.core.windows.net"
		}
	},
	"variables": {
		"workspaceId": "[concat('Microsoft.Synapse/workspaces/', parameters('workspaceName'))]"
	},
